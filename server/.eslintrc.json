{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "global-require": "off",
        "strict": "off",
        "import/no-dynamic-require": "off",
        "prefer-template": "off",
        "no-path-concat": "off",
        "object-shorthand":"off",
        "consistent-return":"off",
        "no-param-reassign":"off"
    }
}
